pushd /data/swiftspace/swiftdrive/
	rm -r bin/*
	cp -r res bin
	cp -r gui bin
	#javac -d bin -sourcepath src:res:gui src/sd/swiftglobal/rk/SwiftStart.java
	find -name "*.java" > sources
	javac -d bin -sourcepath src:res:gui @sources
	pushd /data/swiftspace/swiftdrive/bin
		jar cfm swiftdrive.jar ../manifest.mf sd res
		mv swiftdrive.jar ..
	popd
popd
