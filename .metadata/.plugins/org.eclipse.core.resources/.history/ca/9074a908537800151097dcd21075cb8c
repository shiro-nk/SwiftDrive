package sd.swiftglobal.rk.util;

import java.io.Closeable;

public class Ping implements Runnable, Closeable {
	private boolean active = false;
	
	public void run() {
		
	}
	
	public void deactivate() {
		active = false;
	}
	
	public void activate() {
		active = true;
	}
	
	public void close() {
		
	}
}
