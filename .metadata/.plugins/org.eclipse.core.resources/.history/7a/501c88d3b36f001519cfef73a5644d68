
public class Testing {
	
	public static void main(String[] args) {
		
	}
	
	public class Animal {
		int legs = 0;
		
		Animal(int legs) {
			this.legs = legs;
		}
		
		public int getLegs() {
			return legs;
		}
	}
	
	public class Dog extends Animal {
		String name = "";
		
		Dog(String name, int legs) {
			super(4);
		}
		
		String getName() {
			return name;
		}
	}
}
